<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/nor_bt_press_color"></solid>
           <corners android:radius="@dimen/common4dp"></corners>
        </shape>
    </item>


    <item android:state_pressed="false">

        <shape>
            <solid android:color="@color/nor_bt_color"></solid>
            <corners android:radius="@dimen/common4dp"></corners>
        </shape>

    </item>
</selector>